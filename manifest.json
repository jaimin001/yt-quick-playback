{
	"manifest_version": 3,
	"name": "Youtube Quick Speed",
	"version": "1.0.0",
	"description": "A browser extension to toggle the playback speed to 2x/3x for YouTube videos.",
	"permissions": ["activeTab"],
	"content_scripts": [
		{
			"matches": ["*://*.youtube.com/*"],
			"js": ["script.js"]
		}
	],
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "icons/icon16.png",
			"48": "icons/icon48.png",
			"128": "icons/icon128.png",
			"512": "icons/icon512.png"
		}
	},
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png",
		"512": "icons/icon512.png"
	}
}
